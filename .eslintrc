{
    "parserOptions": {
        "ecmaVersion": 2017
    },
    "rules": {
        "indent": [2, 2],
        "linebreak-style": [2, "unix"],
        "semi": [2, "always"],
        "complexity": [2, 10],
        "no-console": [0],
        "camelcase": [1],
        "eqeqeq": [1]
    },
    "env": {
        "node": true,
        "es6": true
    },
    "extends": "eslint:recommended"
}
